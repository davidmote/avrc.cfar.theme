<?xml version="1.0"?>
<object name="portal_css">

 <!-- Set conditions on stylesheets we don't want to pull in -->
 <stylesheet
     expression="not:request/HTTP_X_THEME_ENABLED | nothing"
     id="public.css"
     />
 <stylesheet
     expression="not:request/HTTP_X_THEME_ENABLED | nothing"
     id="member.css" authenticated="False"
     />

 <stylesheet
     expression="not:request/HTTP_X_THEME_ENABLED | nothing"
     id="dropdown-menu.css" 
     />
 <!-- Add new stylesheets -->
 <!-- Note: applyPrefix is not available in Plone < 4.0b3 -->

 <stylesheet title="" authenticated="False" cacheable="True"
    compression="safe" conditionalcomment="" cookable="True" enabled="on"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    id="++theme++avrc.cfar.theme/theme-css/style_screen.css" media="" rel="stylesheet"
    rendering="link"
    applyPrefix="True"
    />

 <stylesheet title="" authenticated="False" cacheable="True"
    compression="safe" conditionalcomment="" cookable="True" enabled="on"
    expression="request/HTTP_X_THEME_ENABLED | nothing"
    id="++theme++avrc.cfar.theme/theme-css/style_features.css" media="" rel="stylesheet"
    rendering="link"
    applyPrefix="True"
    />

 <stylesheet title="" authenticated="True" cacheable="True"
    compression="safe" conditionalcomment="" cookable="True" enabled="on"
    id="++theme++avrc.cfar.theme/theme-css/edit_screen.css" media="" rel="stylesheet"
    rendering="link"
    applyPrefix="True"
    /> 

</object>